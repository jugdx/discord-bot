{
  "name": "diskord-bot",
  "version": "1.0.0",
  "description": "A discord bot framework for Discord.js",
  "main": "dist/index.js",
  "types": "dist/**/*.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "clean-build": "rimraf dist && tsc",
    "build": "tsc",
    "build-trace": "tsc --traceResolution",
    "watch:build": "tsc --watch",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jugdx/discord-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js"
  ],
  "author": {
    "name": "JuGdx",
    "email": "jugdx@gmail.com",
    "url": "https://github.com/jugdx"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jugdx/discord-bot/issues"
  },
  "homepage": "https://github.com/jugdx/discord-bot#readme",
  "devDependencies": {
    "@types/node": "^14.14.27",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "discord.js": "^12.5.1"
  }
}
